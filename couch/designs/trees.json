{
	"_id": "_design/trees",
	"views": {
		"byId": {
			"map":
				"function (doc) { if (doc._id) { emit(doc._id, doc) } }"
		},
		"checkId": {
			"map":
				"function (doc) { if (doc._id) { emit(doc._id, true) } }"
		},
		"byUserId": {
			"map":
				"function (doc) { if (doc.user && doc.user.id) { emit(doc.user.id, doc) } }"
		}
	}
}
